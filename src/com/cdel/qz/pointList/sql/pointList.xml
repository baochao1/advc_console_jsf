<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pointlist">
	<select id="getPointListList4I" resultType="PointList" parameterType="java.lang.Integer">
		SELECT * from QZ_POINT_LIST WITH (NOLOCK) WHERE courseID=#{value}
    </select>
    
</mapper>